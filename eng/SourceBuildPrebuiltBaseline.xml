    <UsagePattern IdentityGlob="Microsoft.Bcl.AsyncInterfaces/7.0.0" />
    <UsagePattern IdentityGlob="System.Text.Encodings.Web/7.0.0" />
  </IgnorePatterns>
</UsageData>
